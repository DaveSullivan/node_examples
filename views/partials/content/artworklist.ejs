<article class="artworklist">
  <h1>Scenes from trip</h1>
  <p>Check out these pictures from a recent trip to Jamaica.</p>
  <div class="pixgrid">
    <% if (artwork.length > 0) { %>
      <% for (i=0; i<artwork.length; i++) { %>
        <img src="/images/artwork/<%= artwork[i] %>" alt="Artwork <%= i %>">
      <% } %>
    <% } %>
  </div>
</article>